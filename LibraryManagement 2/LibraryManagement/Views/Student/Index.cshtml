@model IEnumerable<LibraryManagement.Models.Student>

@{
	ViewData["Title"] = "Index";
}

<h1>
	Student Index
</h1>

<table class="table">

	<thead>
		<tr>
			<th>id</th>
			<th>name</th>
			<th>address</th>
			<th>mobile</th> 
		</tr>
	</thead>

	<tbody>
		@foreach(var item in Model)
		{
			<tr>
				<td>@item.Id</td>
				<td>@item.Name</td>
				<td>@item.Address</td>
				<td>@item.Mobile</td>
			</tr>
			
		}

	</tbody>

</table>