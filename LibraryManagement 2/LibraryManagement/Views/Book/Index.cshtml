@model IEnumerable<LibraryManagement.Models.Book>

@{
	ViewData["Title"] = "List of Books";

}

<h1>List of Book</h1>

<table class="table">
	<thead>
		<tr>
			<th>Title</th>
			<th>Auther name</th>
			<th>ISBN</th>
		</tr>
	</thead>

	<tbody>
		@foreach(var item in Model)
		{
			<tr>
				<td>@item.Title</td>
				<td>@item.Auther</td>
				<td>@item.ISBN</td>

			</tr>
		}
	</tbody>

</table>